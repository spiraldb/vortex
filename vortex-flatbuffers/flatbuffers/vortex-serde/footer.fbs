table FlatLayout {
    begin: uint64;
    end: uint64;
}

table NestedLayout {
    children: [Layout];
    encoding: uint16;
}

union LayoutVariant {
    FlatLayout,
    NestedLayout,
}

table Layout {
    layout: LayoutVariant;
}

table Footer {
    layout: Layout;
    row_count: uint64;
}

root_type Footer;